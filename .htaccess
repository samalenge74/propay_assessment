<IfModule mod_rewrite.c>
        RewriteEngine On
        RewriteBase /
        # Directs all EE web requests through the site index file
        RewriteCond %{REQUEST_FILENAME} !-f
        RewriteCond %{REQUEST_FILENAME} !-d
        RewriteRule ^(.*)$ /Propay_Assessment/index.php/$1 [L]
</IfModule>
